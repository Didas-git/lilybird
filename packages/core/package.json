{
  "name": "lilybird",
  "version": "0.3.0",
  "description": "A bun-first discord client written in TS",
  "main": "./dist/index.js",
  "author": "DidaS",
  "license": "Apache-2.0",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "engines": {
    "bun": ">=1.0.15",
    "node": ">=21.0.0"
  },
  "files": [
    "./dist"
  ],
  "scripts": {
    "check": "eslint \"src/**/*.ts\"",
    "build": "rm -rf dist && tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Didas-git/lilybird"
  },
  "homepage": "https://github.com/Didas-git/lilybird/tree/main/packages/core",
  "bugs": {
    "url": "https://github.com/Didas-git/lilybird/issues"
  },
  "keywords": [
    "lilybird",
    "discord",
    "wrapper",
    "client",
    "discord-client",
    "discord-wrapper",
    "bun",
    "bunjs",
    "bun-first"
  ],
  "devDependencies": {
    "eslint": "^8.55.0",
    "@typescript-eslint/parser": "^6.13.2",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@stylistic/eslint-plugin": "^1.4.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "bun-types": "^1.0.15",
    "typescript": "^5.3.2"
  }
}