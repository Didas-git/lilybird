{
  "name": "lilybird",
  "version": "0.2.4",
  "description": "A bun-first discord client written in TS",
  "main": "./dist/index.js",
  "author": "DidaS",
  "license": "Apache-2.0",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "engines": {
    "bun": ">=1.0.14",
    "node": ">=21.0.0"
  },
  "files": [
    "./dist"
  ],
  "scripts": {
    "lint": "biome lint .",
    "format": "biome format .",
    "check": "biome check --verbose .",
    "build": "rm -rf dist && tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Didas-git/lilybird"
  },
  "homepage": "https://github.com/Didas-git/lilybird/tree/main/packages/core",
  "bugs": {
    "url": "https://github.com/Didas-git/lilybird/issues"
  },
  "keywords": [
    "lilybird",
    "discord",
    "wrapper",
    "client",
    "discord-client",
    "discord-wrapper",
    "bun",
    "bunjs",
    "bun-first"
  ],
  "devDependencies": {
    "@biomejs/biome": "^1.4.1",
    "bun-types": "^1.0.14",
    "typescript": "^5.3.2"
  }
}