{
  "name": "@lilybird/jsx",
  "version": "0.1.0",
  "description": "JSX support & builders for lilybird",
  "main": "./dist/index.js",
  "author": "DidaS",
  "license": "Apache-2.0",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./jsx-runtime": {
      "types": "./dist/runtime/index.d.ts",
      "require": "./dist/runtime/index.js",
      "default": "./dist/runtime/index.js"
    },
    "./package.json": "./package.json"
  },
  "engines": {
    "bun": ">=1.0.14"
  },
  "files": [
    "./dist"
  ],
  "scripts": {
    "lint": "eslint src/**/*.ts",
    "build": "rm -rf dist && tsc",
    "build:lint": "bun run lint && bun run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Didas-git/lilybird"
  },
  "homepage": "https://github.com/Didas-git/lilybird/tree/main/packages/core",
  "bugs": {
    "url": "https://github.com/Didas-git/lilybird/issues"
  },
  "keywords": [
    "discord",
    "wrapper",
    "client",
    "discord-client",
    "discord-wrapper",
    "bun",
    "bun-first"
  ],
  "peerDependencies": {
    "lilybird": "workspace:*"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.4.1",
    "bun-types": "^1.0.14",
    "typescript": "^5.3.2"
  }
}